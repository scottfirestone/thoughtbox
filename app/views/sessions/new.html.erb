<h1>Login</h1>
<%= form_for(:session, url: login_path) do |f| %>
  <h4><%= f.label :email %></h4>
  <h4><%= f.text_field :email %></h4>
  <h4><%= f.label :password %></h4>
  <h4><%= f.password_field :password %></h4>
  <%= f.submit "Login", class: "btn btn-primary btn-font-size" %><br>
  <br>
  <%= link_to "Register", new_user_path, class: "btn btn-primary btn-font-size" %>
<% end %>
